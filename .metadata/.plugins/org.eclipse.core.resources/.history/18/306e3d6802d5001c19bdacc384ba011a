package Practice;

public class P1 {

	public static void main(String[] args) {
		int[] arr = new int[5];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
			System.out.println(arr[i]);
		}
		
		int[] arr2 = new int [arr.length*2];
		for(int i : arr2) {
			arr2[i] = arr[i];
			System.out.print(arr2[i]);
		}
		
		for(int i : arr2) {
			System.out.print(arr2[i]);
		}
		
	}

}
