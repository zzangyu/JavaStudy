package pratice;

import java.util.Scanner;

public class asd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		while (true) {
			System.out.println("GOGO");
			if (!sc.nextlLine().equals("N") || !sc.next().equals("n"))
				break;
		}
		System.out.println("STOP");
	}

}
