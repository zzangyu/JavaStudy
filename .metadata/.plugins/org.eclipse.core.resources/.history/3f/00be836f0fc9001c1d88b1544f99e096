package HelloWorld;

public class MyCharRunner {

	public static void main(String[] args) {
		MyChar myChar = new MyChar('z');
		System.out.println("알파벳입니까?"+myChar.isVowel()); //모음
//		myChar.isConsonant(); // 자음
		System.out.println("0~9 사이입니까?"+myChar.isDigit());
		System.out.println("알파벳인가요?"+myChar.isAlphaber());
//		MyChar.printLowerCaseAlphabets();
//		MyChar.printUpperCaseAlphabets();

	}

}
