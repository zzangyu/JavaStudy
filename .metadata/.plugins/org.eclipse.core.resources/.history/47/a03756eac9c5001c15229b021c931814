package study;

public class dwq {

	public static void main(String[] args) {
		
		int i, sum1=0, sum2=0;
		  for(i=0; i<11; i++){
		  if(i/2==0){
		  	sum1 = sum1+i;
		  	System.out.println(i+","+sum1);
		  }
		  else {
		  	sum1 = sum1+i;
		  	System.out.println(i+","+sum1);
		  }
		  }
		 
	}

}
