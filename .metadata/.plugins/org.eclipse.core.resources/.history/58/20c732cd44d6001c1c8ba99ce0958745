package Method;

public class MethodEx02_1 {

	public static void star() {
		for (int a = 0; a < 5; a++) {
			System.out.print("*");
		}
	}

	public static void main(String[] args) {
		star();
		System.out.println("Hello");
		star();
		System.out.println("Java");
		star();
		System.out.println("!!!!");
	}

}
