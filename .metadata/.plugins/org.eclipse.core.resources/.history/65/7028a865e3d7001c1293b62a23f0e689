package com.constex;

class BookEx03 {
	String title;
	String author;

	public void show() {
		System.out.println(title + " " + author);
	}

	public BookEx03() {
		this(string, string);
	}

	public BookEx03(String title) {
		this.title = title;
	}

	public BookEx03(String title, String author) {
		this.title = title;
		this.author = author;

	}
}

public class ThisEx03 {

	public static void main(String[] args) {

	}

}
