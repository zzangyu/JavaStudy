package com.constex;

class ThisEx {

	String name, id, tel;

	public ThisEx() {
		this.name = "Guest";
		this.id = "123456-1234567";
		this.tel = "010-4140-5191";
	}

	public ThisEx(String name) {
		this();
		this.name = name;
	}

	public ThisEx(String name, String id) {
		this(name);
		this.id = id;
	}

	public ThisEx(String name, String id, String tel) {
		this(name, id);
		this.tel = tel;
	}

}

public class ThisEx04 {

	public static void main(String[] args) {
		ThisEx t = new ThisEx();
		System.out.println("Name: "+t.name);
		System.out.println("Id: "+t.id);
		System.out.println("Tel: "+t.tel);
		ThisEx t1 = new ThisEx();
		ThisEx t2 = new ThisEx();
		ThisEx t3 = new ThisEx();
		
	}

}
