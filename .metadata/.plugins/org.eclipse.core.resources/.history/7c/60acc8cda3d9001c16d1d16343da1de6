package ExceptionHandling;

import java.util.Scanner;

public class ExceptionQuizEx01 {

	public static void main(String[] args) {
		Scanner sc = null;
		try {
			sc = new Scanner(System.in);
			String str = sc.next();
			str.toString();
		} catch (Exception e) {

		} finally {
			sc.close();
		}
	}

}
