package practice;

class Student3 {
	private int id;
	private String name;

	public Student3(int id, String name) {
		super();
		this.id = id;
		this.name = name;

	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}

public class Collections2 {

	public static void main(String[] args) {

	}

}
