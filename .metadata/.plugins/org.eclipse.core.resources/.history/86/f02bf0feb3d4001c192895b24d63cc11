package Array;

public class Arraycopy2 {

	public static void main(String[] args) {
		int[] arr = new int[5];

		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
		}

		int[] arr2 = new int[10];
		for (int i = 0; i < arr.length; i++) {
			arr2[i + 3] = arr[i];
			System.out.println(arr[i]);
		}
	}

}
