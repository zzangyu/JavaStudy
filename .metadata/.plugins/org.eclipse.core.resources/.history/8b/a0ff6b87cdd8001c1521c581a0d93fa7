package Thread;

public class TreadBasicsRunner {

	public static void main(String[] args) {
		// Task 1
		for (int i = 0; i < 9; i++) {
			System.out.print(i);
		}
		System.out.println("\nTask 1 Done");
		// Task 2
		for (int i = 9; i < 19; i++) {
			System.out.print(i);
		}
		System.out.println("\nTask 2 Done");
		// Task 3
		for (int i = 19; i < 29; i++) {
			System.out.print(i);
		}
		System.out.println("\nTask 3 Done");
		System.out.println("Main Done");
	}

}
