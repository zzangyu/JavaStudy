package Method;

public class MethodEx02_1 {

	public static void star() {
		for (int a = 0; a < 5; a++) {
			System.out.print("*");
		}
	}

	public static void main(String[] args) {
		star();
		System.out.println("\nHello");
		star();
		System.out.println("\nJava");
		star();
		System.out.println("\n!!!!");
	}

}
