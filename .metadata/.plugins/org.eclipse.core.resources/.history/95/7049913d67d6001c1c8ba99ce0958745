package pizza;

public class CircleEx {

	public static void main(String[] args) {
		Circle pizza; // reference 변수선언

		pizza = new Circle(); // 객체 생성

		// 반지름 10
		pizza.radius = 10;
		pizza.name = "초콜릿 피자";

		double area = pizza.getArea(10);
		System.out.println();
		Circle.getAround(pizza.radius);

	}

}
