package pratice;

import java.util.Scanner;

public class asd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		while (true) {
			System.out.println("GOGO");
			String str = sc.next();
			sc.nextLine();
			if (!str.equals("n") || !str.equals("N"))
				break;
		}
		System.out.println("STOP");
	}

}
