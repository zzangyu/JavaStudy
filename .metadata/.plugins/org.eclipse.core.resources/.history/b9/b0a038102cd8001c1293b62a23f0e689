package pratice;

import java.util.Scanner;

public class asd {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		while (true) {
			System.out.println("GOGO");
			String s = sc.next();
			if (!s.equals("N") || !s.equals("n"))
				break;
		}
		System.out.println("STOP");
	}

}
