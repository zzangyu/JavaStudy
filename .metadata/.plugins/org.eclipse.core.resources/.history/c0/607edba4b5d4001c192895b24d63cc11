package Array;

public class Arraycopy2 {

	public static void main(String[] args) {
		int[] arr = new int[5];
		int[] arr33 = new int[2];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
		}

		int[] temp = new int[arr.length * 2];
		for (int i = 0; i < arr.length; i++) {
			temp[i] = arr[i];
			System.out.print(temp[i] + "\t");

		}
		System.out.println();
		arr33 = temp;
		System.out.println("º¯°æ ÈÄ .. arr.length: " + arr33.length);
		for (int i : arr33) {
			System.out.println(i);
		}
	}

}
