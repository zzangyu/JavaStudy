package com.constex;

class ThisEx {

	String name, id, tel;

	public ThisEx() {
		this.name = "Guest";
		this.id = "123456-1234567";
		this.tel = "010-4140-5191";
	}

	public ThisEx(String name) {
		this(name, "654321-7654321", "1915-0414-010");
		this.name = name;
	}

	public ThisEx(String name, String id) {
		this(" ");
		this.id = id;
	}

}

public class ThisEx04 {

	public static void main(String[] args) {

	}

}
