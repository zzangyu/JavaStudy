package Array;

public class ArrayEx01 {

	public static void main(String[] args) {
		int aa[][] = new int[3][4];
		
		for(int i = 0; i<=aa.length-1; i++) {
			for(int j = 0; j<=aa[i].length-1; j++) {
				int a = 0;
				aa[i][j] = a;
				System.out.print(aa[i][j]);
				a++;
			}
			System.out.println();
		} 
		
		
		
	}

}
