package com.constex;

class ThisEx {

	String name, id, tel;

	public ThisEx() {
		this.name = "Guest";
		this.id = "000000-0000000";
		this.tel = "000-0000-0000";
	}

	public ThisEx(String name) {
		this();
		this.name = name;
	}

	public ThisEx(String name, String id) {
		this(name, id, "");
		this.name = name;
		this.id = id;
	}

	public ThisEx(String name, String id, String tel) {
		this.name = name;
		this.id = id;
		this.tel = tel;
	}

	public show() {
		System.out.println(name+"//"+id+"//"+tel);
	}
}

public class ThisEx04 {

	public static void main(String[] args) {
		ThisEx t = new ThisEx();
		System.out.println("Name: " + t.name);
		System.out.println("Id: " + t.id);
		System.out.println("Tel: " + t.tel);
		ThisEx t1 = new ThisEx("한찬규");
		System.out.println("Name: " + t1.name);
		System.out.println("Id: " + t1.id);
		System.out.println("Tel: " + t1.tel);
		ThisEx t2 = new ThisEx("사토미", "555555-5555555");
		System.out.println("Name: " + t2.name);
		System.out.println("Id: " + t2.id);
		System.out.println("Tel: " + t2.tel);
		ThisEx t3 = new ThisEx("아이유", "777777-7777777", "777-7777-7777");
		System.out.println("Name: " + t3.name);
		System.out.println("Id: " + t3.id);
		System.out.println("Tel: " + t3.tel);

	}

}
