package Class;

public class AccountEx {

	public static void main(String[] args) {
		Account ac = new Account();
		ac.setMoney(1000000, "1234");
		int myPay = ac.getMoney();
		System.out.printf("나의 현재 잔액: %d원\n", myPay);

		ac.donjo(1, "1234");
		myPay = ac.getMoney();
		System.out.printf("출금 후 잔액: %d원", myPay);

		ac.donmogo(10000000);
		myPay = ac.getMoney();
		System.out.printf("입금 후 잔액: %d원", myPay);
	}

}
