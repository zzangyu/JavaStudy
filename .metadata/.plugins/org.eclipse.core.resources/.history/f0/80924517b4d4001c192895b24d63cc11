package Array;

public class Arraycopy2 {

	public static void main(String[] args) {
		int[] arr = new int[5];

		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
		}

		int[] arr2 = new int[10];
		for (int i = 0; i < arr.length; i++) {
			arr2[i + 3] = arr[i];
		}
		for (int i : arr2) {
			System.out.println(i);
		}

		int[] arr3 = new int[5];
		arr3 = {1, 2, 3};
	}

}
