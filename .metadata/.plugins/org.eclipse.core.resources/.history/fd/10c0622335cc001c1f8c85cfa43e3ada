package 과제;

public class 구구단 {

	public static void main(String[] args) {
		int k, i, j;
		for (i = 2; i < 10; i += 3) {
			for (j = 1; j < 10; j++) {
				for (k = i; k < i + 3; k++)
					if(k!=10) {
					System.out.print(k + "X" + j + "=" + k * j + "\t");
					}
					System.out.println();

			}
			System.out.println();

		}

	}

}
